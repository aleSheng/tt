{
  "name": "@tagtime/cli",
  "version": "0.1.3",
  "description": "TagTime CLI - Save and search notes from terminal",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "tt": "./dist/index.js"
  },
  "files": [
    "dist",
    "SKILL.md",
    "scripts/postinstall.js"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean",
    "build:standalone": "bun build src/index.ts --compile --outfile bin/tt",
    "build:all": "pnpm build && pnpm build:standalone",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build && pnpm run test",
    "postinstall": "node scripts/postinstall.js || true",
    "publish:npm": "npm publish --access public",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "minisearch": "^7.2.0",
    "open": "^11.0.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vitest": "^3.1.4"
  },
  "keywords": [
    "tagtime",
    "cli",
    "notes",
    "knowledge-base"
  ],
  "license": "MIT"
}
